{
  "name": "node-requirer",
  "version": "0.1.12",
  "description": "Node-requirer lets you quickly add require/import statements to any files/node_modules within your code!",
  "keywords": [
    "atom",
    "node",
    "require",
    "import"
  ],
  "license": "MIT",
  "main": "./lib/main",
  "repository": "https://github.com/tnrich/atom-node-requirer",
  "dependencies": {
    "async": "0.2.6",
    "atom-space-pen-views": "^2.0.0",
    "camelcase": "^3.0.0",
    "filehound": "^1.7.0",
    "filename-to-module-name": "^0.1.1",
    "find-node-modules": "^1.0.1",
    "fs-plus": "^2.0.0",
    "fuzzaldrin": "^2.0",
    "fuzzaldrin-plus": "^0.1",
    "humanize-plus": "~1.4.2",
    "minimatch": "~0.3.0",
    "node-dir": "^0.1.12",
    "npmls": "^3.0.0",
    "path-exists": "^3.0.0",
    "relative": "^3.0.2",
    "temp": "~0.8.1",
    "underscore-plus": "^1.0.0",
    "walk-sync": "^0.3.0",
    "wrench": "^1.5"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationCommands": {
    "atom-workspace": [
      "node-requirer:require",
      "node-requirer:import"
    ]
  }
}
